<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 name="panel_profile"
 label="Profile"
 top="0"
 left="0"
 height="480"
 width="420"
 follows="all"
 layout="topleft"
>
    <string
     name="status_online"
    >
        Currently Online
    </string>
    <string
     name="status_offline"
    >
        Currently Offline
    </string>
    <string
     name="CaptionTextAcctInfo"
    >
Acount: [ACCTTYPE]
[PAYMENTINFO]
    </string>
    <string
     name="payment_update_link_url"
    >
        http://www.secondlife.com/account/billing.php?lang=en
    </string>
    <string
     name="partner_edit_link_url"
    >
        http://www.secondlife.com/account/partners.php?lang=en
    </string>
    <string
     name="my_account_link_url"
     value="http://secondlife.com/account"
    />
    <string
     name="no_partner_text"
     value="None"
    />
    <string
     name="no_group_text"
     value="None"
    />
    <string
     name="RegisterDateFormat"
    >
        [REG_DATE]
    </string>
    <string
     name="name_text_args"
    >
        [NAME]
    </string>
    <string
     name="display_name_text_args"
    >
        [DISPLAY_NAME]
    </string>
    <string
     name="FSDev"
     value=" Developer"
    />
    <string
     name="FSSupp"
     value=" Support"
    />
    <string
     name="FSQualityAssurance"
     value=" Bug Hunter"
    />
    <string
     name="FSGW"
     value=" Gateway"
    />
    <loading_indicator
     name="progress_indicator"
     top="4"
     right="-10"
     height="23"
     width="23"
     follows="top|right"
     layout="topleft"
     visible="false"
    />
    <layout_stack
     name="imagepositioner"
     top="8"
     left="6"
     right="-1"
     height="158"
     follows="top|left|right"
     layout="topleft"
     orientation="horizontal"
    >
        <layout_panel
         name="image_stack"
         follows="all"
         layout="topleft"
         width="158"
         height="158"
         auto_resize="false"
         user_resize="false"
        >
            <!-- 23 pixels (BTN_HEIGHT_SMALL) are reserved by label field of texture and shouldn't be visible-->
            <!-- Todo: should become non clickable?-->
            <texture_picker
             name="2nd_life_pic"
             top="0"
             left="0"
             right="-1"
             height="180"
             follows="all"
             layout="topleft"
             allow_no_texture="true"
             default_image_name="None"
             fallback_image="Generic_Person_Large"
             />
        </layout_panel>
        <layout_panel
         name="label_stack"
         follows="all"
         layout="topleft"
         width="200"
         auto_resize="true"
         user_resize="false"
        >
            <text
             name="display_name"
             value="(loading...)"
             top="1"
             left="4"
             right="-6"
             height="18"
             follows="top|left|right"
             layout="topleft"
             font="SansSerifLarge"
            />
            <text
             name="agent_name"
             value=""
             top_pad="2"
             left="4"
             right="-6"
             height="16"
             follows="top|left|right"
             layout="topleft"
            />
            <text
             name="register_date"
             value=""
             top_pad="2"
             left="4"
             right="-6"
             height="16"
             follows="top|left|right"
             layout="topleft"
             />
            <!-- Todo: url either from code or format like payment info?-->
            <text
             name="partner_text"
             value=""
             top_pad="2"
             left="4"
             right="-6"
             height="16"
             follows="top|left|right"
             layout="topleft"
             />
            <!-- Populates with CaptionTextAcctInfo-->
            <text
             name="acc_status_text"
             top_pad="2"
             left="4"
             right="-6"
             height="44"
             follows="top|left|right"
             layout="topleft"
             allow_scroll="false"
             word_wrap="true"
             />
            <text
             name="status"
             value="Status Unknown"
             top_pad="2"
             left="4"
             right="-6"
             height="16"
             follows="top|left"
             layout="topleft"
             halign="center"
             text_color="LtGray_50"
             />
            <menu_button
             name="copy_btn"
             tool_tip="Copy options"
             top_pad="2"
             left="4"
             width="120"
             label="Actions"
             halign="left"
             hover_glow_amount="0.33"
             image_unselected="DropDown_Off"
             image_selected="DropDown_On"
             image_pressed="DropDown_Press"
             image_pressed_selected="DropDown_Press"
             image_disabled="DropDown_Disabled"
             follows="top|left"
             layout="topleft" />
        </layout_panel>
        <layout_panel
         name="owner_button_stack"
         follows="all"
         layout="topleft"
         width="12"
         auto_resize="false"
         user_resize="false"
        >
          <button
           name="edit_display_name"
           label="Edit Display Name"
           top="1"
           left="0"
           height="20"
           width="55"
           follows="top|left"
           layout="topleft"
          />
          <button
           name="edit_partner"
           label="Edit partner"
           top_pad="5"
           left_delta="0"
           height="20"
           width="55"
           follows="top|left"
           layout="topleft"
          />
          <button
           name="change_photo"
           label="Edit partner"
           top_pad="5"
           left_delta="0"
           height="20"
           width="55"
           follows="top|left"
           layout="topleft"
          />
          <button
           name="remove_photo"
           label="Edit partner"
           top_pad="5"
           left_delta="0"
           height="20"
           width="55"
           follows="top|left"
           layout="topleft"
          />
        </layout_panel>
        <layout_panel
         name="permissions_button_stack"
         follows="all"
         layout="topleft"
         width="8"
         auto_resize="false"
         user_resize="false"
        >
          <!-- Todo: This needs to contain clickable icons, icons not avaliable yet -->
        </layout_panel>
    </layout_stack>

    <layout_stack
     name="about_positioner"
     top_pad="7"
     left="6"
     right="-6"
     height="290"
     follows="all"
     layout="topleft"
     orientation="vertical"
    >
      <layout_panel
       name="about_stack"
       follows="all"
       layout="topleft"
       left="0"
       height="105"
       auto_resize="true"
       user_resize="false">
        <text_editor
         name="sl_description_edit"
         trusted_content="false"
         always_show_icons="true"
         enabled="false"
         top="0"
         left="6"
         right="-1"
         bottom="-1"
         follows="all"
         layout="topleft"
         bg_readonly_color="Transparent"
         border_visible="true"
         font="SansSerifSmall"
         h_pad="2"
         max_length="510"
         parse_urls="true"
         word_wrap="true"
        />
      </layout_panel>
      <layout_panel
       name="about_edit_stack"
       follows="all"
       layout="topleft"
       left="0"
       height="22"
       auto_resize="false"
       user_resize="false">
        <button
         name="about_discard"
         label="Discard changes"
         top="1"
         right="-1"
         height="20"
         width="120"
         follows="top|right"
         layout="topleft"
          />
        <button
         name="about_save"
         label="Save"
         top="1"
         right="-125"
         height="20"
         width="120"
         follows="top|right"
         layout="topleft"
          />
      </layout_panel>
      <layout_panel
       name="interests_group_stack"
       follows="all"
       layout="topleft"
       left="0"
       height="110"
       auto_resize="true"
       user_resize="false">
        <text
         name="interests_label"
         value="Interests"
         top="7"
         left="6"
         height="16"
         width="170"
         follows="left|top"
         layout="topleft"
        />
        <button
         name="delete_interest_btn"
         tool_tip="Delete selected interest"
         height="16"
         left="180"
         top_delta="0"
         width="16"
         image_selected="TrashItem_Press"
         image_unselected="TrashItem_Off"
         image_disabled="TrashItem_Disabled"
         follows="left|top"
         layout="topleft" />
        <scroll_list
         name="interest_list"
         top_pad="4"
         left="6"
         height="83"
         width="190"
         follows="left|top|bottom"
         layout="topleft"
         border_visible="true"
         color="ScrollBgWriteableColor"
         for_agent="false"
        />
        <text
         name="groups_list_label"
         value="Groups memberships"
         top="7"
         left_pad="16"
         height="16"
         width="180"
         follows="left|top"
         layout="topleft"
        />
        <group_list
         name="group_list"
         top_pad="4"
         left_delta="0"
         right="-7"
         height="83"
         follows="left|top|right|bottom"
         layout="topleft"
         border_visible="true"
         color="ScrollBgWriteableColor"
         for_agent="false"
        />
        <text
         name="membership_label"
         value="My groups memberships"
         top="7"
         left_delta="0"
         height="16"
         width="180"
         follows="left|top"
         layout="topleft"
        />
        <combo_box
         allow_text_entry="false"
         height="22"
         left_delta="0"
         top_pad="3"
         width="180"
         follows="left|top"
         layout="topleft"
         name="membership_display">
          <combo_box.item
           label="Everyone can see"
           name="item1" />
        </combo_box>
        <text
         name="profile_search_label"
         value="Show my profile in search"
         top_pad="17"
         left_delta="0"
         height="16"
         width="180"
         follows="left|top"
         layout="topleft"
        />
        <combo_box
         allow_text_entry="false"
         height="22"
         left_delta="0"
         top_pad="3"
         width="180"
         follows="left|top"
         layout="topleft"
         name="search_display">
          <combo_box.item
           label="Show"
           name="item1" />
        </combo_box>
      </layout_panel>
      <layout_panel
       name="interests_edit_stack"
       follows="all"
       layout="topleft"
       left="0"
       height="20"
       auto_resize="false"
       user_resize="false">
        <line_editor
            border_style="line"
            border_thickness="1"
            left="6"
            height="23"
            top="1"
            width="113"
            follows="top|left"
            layout="topleft"
            name="log_path_string"
            font="SansSerif"
            max_length_bytes="256">
        </line_editor>
        <button
         name="add_interest"
         label="Add"
         top="1"
         left_pad="7"
         height="20"
         width="70"
         follows="top|left"
         layout="topleft"
          />
      </layout_panel>
      <layout_panel
       name="give_stack_to_remove"
       follows="left|bottom"
       layout="topleft"
       left="0"
       height="24"
       auto_resize="false"
       user_resize="false">
        <text
         name="Give item:"
         value="Give item:"
         top="6"
         left="0"
         height="16"
         width="55"
         follows="left|bottom"
         layout="topleft"
         halign="right"
        />
        <view_border
         name="drop_target_rect_vis"
         top_delta="0"
         left="60"
         right="-1"
         height="16"
         follows="left|bottom|right"
         layout="topleft"
         bevel_style="in"
        />
        <text
         name="Give inventory"
         tool_tip="Drop inventory items here to give them to this person."
         top_delta="0"
         left="60"
         right="-1"
         height="16"
         follows="left|bottom|right"
         layout="topleft"
         halign="center"
        >
          Drop inventory item here.
        </text>
      </layout_panel>
    </layout_stack>
  
    <profile_drop_target
     name="drop_target"
     top="0"
     bottom="-1"
     left="0"
     right="-1"
     follows="all"
     layout="topleft"
     mouse_opaque="false"
    />
</panel>
